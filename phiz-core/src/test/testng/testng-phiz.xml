<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="%{project.name} Tests Suite" verbose="1">
    <test name="%{project.name} Tests">
        <groups>
            <run>
                <include name="phiz\.test\.all"/>
                <exclude name="phiz\.test\.it\.all"/>
            </run>
        </groups>
        <packages>
            <package name="gov.hhs.onc.phiz.*"/>
        </packages>
    </test>
</suite>