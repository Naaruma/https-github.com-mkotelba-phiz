<?xml version="1.0" encoding="UTF-8"?>
<con:soapui-settings xmlns:con="http://eviware.com/soapui/config">
    <!--====================================================================================================
    = SETTINGS: GLOBAL PROPERTIES
    =====================================================================================================-->
    <con:setting id="GlobalPropertySettings@enableOverride">true</con:setting>
    
    <!--====================================================================================================
    = SETTINGS: HTTP
    =====================================================================================================-->
    <con:setting id="HttpSettings@close-connections">true</con:setting>
    <con:setting id="HttpSettings@include_request_in_time_taken">true</con:setting>
    <con:setting id="HttpSettings@include_response_in_time_taken">true</con:setting>
    
    <!--====================================================================================================
    = SETTINGS: WS-ADDRESSING
    =====================================================================================================-->
    <con:setting id="WsaSettings@overrideExistingHeaders">true</con:setting>
    <con:setting id="WsaSettings@useDefaultRelatesTo">false</con:setting>
    <con:setting id="WsaSettings@useDefaultRelationshipType">false</con:setting>
    
    <!--====================================================================================================
    = SETTINGS: WSDL
    =====================================================================================================-->
    <con:setting id="WsdlSettings@cache-wsdls">false</con:setting>
    <con:setting id="WsdlSettings@pretty-print-project-files">true</con:setting>
    <con:setting id="WsdlSettings@pretty-print-response-xml">true</con:setting>
    <con:setting id="WsdlSettings@type-comment">true</con:setting>
</con:soapui-settings>