"/%{project.build.debLogDataDirectoryPath}/*.log" {
    copytruncate
    create 640 %{project.artifactId} adm
    daily
    dateext
    dateformat .%Y-%m-%d
    extension .log
    missingok
    nocompress
    postrotate
        chmod 640 "$1"
        chown "%{project.artifactId}:adm" "$1"
    endscript
    rotate 51
    sharedscripts
}
